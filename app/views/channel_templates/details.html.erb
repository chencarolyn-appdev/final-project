<h1>
  <%= @channel.name %> Channel 
</h1>

<div>
  <div>
    <a href="/channels">
      Go back
    </a>
  </div>

  <div>
    <a href="/existing_channel_form/<%= @channel.id %>">
      Edit channel
    </a>
  </div>

  <div>
    <a href="/delete_channel/<%= @channel.id %>">
      Delete channel
    </a>
  </div>
</div>

<dl>
  <dt>
    Name
  </dt>
  <dd>
    <%= @channel.name %>
  </dd>

  <dt>
    Channel Number
  </dt>
  <dd>
    <%= @channel.channel_number %>
  </dd>

  <dt>
    Channel Shows
  </dt>
  <dd>
    <%= @channel.shows.join(", ") %>
  </dd>
 
</dl>

